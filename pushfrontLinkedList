#include<iostream>
using namespace std;

class ListNode{
public:
    int value;
    ListNode* next;
    ListNode(int val) : value (val) , next (nullptr){}
};

class List{
public:
    ListNode* head = nullptr;

    void push_front(int val){
        ListNode* newNode = new ListNode(val);
            if(head == nullptr){
            head = newNode;
        }else{
            newNode->next = head;
            head = newNode;
        }
    }

    void traversal(){
        ListNode* temp = head;
        while(temp != NULL){
            cout<<temp->value<<" ";
            temp = temp->next;
        }
    }
};


int main(){
    List ll;
    ll.push_front(1);
    ll.push_front(2);
    ll.push_front(3);

    ll.traversal();

}
